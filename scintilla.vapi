	[CCode(type_check_function="scintilla_get_type")]
	public class Object : Gtk.Container
	{
		
		[CCode(type="GtkWidget*",has_construct_function=false,
			cname="scintilla_new")]
		public Object();
		
		/**
		 * Set the control ID which will be used in the idFrom field of
		 * the NotifyHeader structure of all notifications for this 
		 * instance. When an application creates multiple Scintilla
		 * widgets, this allows the source of each notification to be 
		 * found. The value should be small, preferrably less than 16 
		 * bits, rather than a pointer as some of the functions will
		 * only transmit 16 or 32 bits.
		 * 
		 * @id: The control ID to set for this Object.
		 */
		[CCode(cname="scintilla_set_id")]
		public void set_id ([CCode(type="uptr_t")]uint id);
		
		/**		 
		 * send_message:
		 * @iMessage: 	The message to send as described in the
		 * 				Scintilla documentation.
		 * @wParam: 	First parameter for message.
		 * @lParam: 	Second parameter for message.
		 * 
		 * The main entry point allows sending any of the messages 
		 * described at: http://www.scintilla.org/ScintillaDoc.html.
		 * If either parameter isn't used, set it to 0.
		 * 
		 * Returns: Various values as described in above link.
		 */
		[CCode(cname="scintilla_send_message")]
		public long send_message([CCode(type="unsigned int")]Messages iMessage, 
									[CCode(type="uptr_t")]ulong wParam=0, 
									[CCode(type="sptr_t")]long lParam=0);
		
		/**
		 * release_resources:
		 * 
		 * Call this to free any remaining resources after all the 
		 * Scintilla widgets have been destroyed.
		 */
		[CCode(cname="scintilla_release_resources")]
		public void release_resources();

        public signal void sci_notify(int param, 
			[CCode(type="gpointer")]Scintilla.Notification* notif);
			//[CCode(type="gpointer")]Scintilla.Object sci);
        	                    
	}

	
	[CCode(cname="struct NotifyHeader")]
	public struct NotifyHeader
	{
		void *hwndFrom;
		ulong idFrom;
		uint code;
	}		

	[CCode(cname="struct SCNotification")]
	public struct Notification 
	{
		Scintilla.NotifyHeader nmhdr;
		int position;
		// SCN_STYLENEEDED, SCN_DOUBLECLICK, SCN_MODIFIED, SCN_DWELLSTART,
		// SCN_DWELLEND, SCN_CALLTIPCLICK,
		// SCN_HOTSPOTCLICK, SCN_HOTSPOTDOUBLECLICK, SCN_HOTSPOTRELEASECLICK
		int ch;             // SCN_CHARADDED, SCN_KEY
		int modifiers;
		// SCN_KEY, SCN_DOUBLECLICK, SCN_HOTSPOTCLICK, SCN_HOTSPOTDOUBLECLICK, SCN_HOTSPOTRELEASECLICK
		int modificationType; // SCN_MODIFIED
		string text;        // SCN_MODIFIED, SCN_USERLISTSELECTION, SCN_AUTOCSELECTION
		int length;         // SCN_MODIFIED
		int linesAdded;     // SCN_MODIFIED
		int message;        // SCN_MACRORECORD
		ulong wParam;       // SCN_MACRORECORD
		long lParam;        // SCN_MACRORECORD
		int line;           // SCN_MODIFIED, SCN_DOUBLECLICK
		int foldLevelNow;   // SCN_MODIFIED
		int foldLevelPrev;  // SCN_MODIFIED
		int margin;         // SCN_MARGINCLICK
		int listType;       // SCN_USERLISTSELECTION, SCN_AUTOCSELECTION
		int x;              // SCN_DWELLSTART, SCN_DWELLEND
		int y;              // SCN_DWELLSTART, SCN_DWELLEND
	}
	
